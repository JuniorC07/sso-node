{
  "name": "node",
  "version": "1.0.0",
  "description": "Simple Project Node",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/index.ts' -e .ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "migrate:make": "yarn knex migrate:make --knexfile src/providers/knex/knexfile.ts", 
    "migrate": "yarn knex migrate:up --knexfile src/providers/knex/knexfile.ts", 
    "migrate:all": "yarn knex migrate:latest --knexfile src/providers/knex/knexfile.ts", 
    "migrate:rollback": "yarn knex migrate:rollback --knexfile src/providers/knex/knexfile.ts", 
    "migrate:list": "yarn knex migrate:list --knexfile src/providers/knex/knexfile.ts", 
    "commit": "npm run lint && git-cz"
  },
  "keywords": [],
  "author": {
    "name": "Junior Camargo",
    "email": "juniorc.dev@gmail.com",
    "url": "https://jrcamargo.dev/"
  },
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "knex": "^2.4.2",
    "pg": "^8.10.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^4.10.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "type": "commonjs"
}
